.row, .column
	box-sizing: border-box
.row:after,.row:before
	content: " "
	display: table
.row:after
	clear:both
.column
	position: relative
	float: left
	display:block
.column + .column
	margin-left: 1.5%
.col-1
	width: 6.95833333333%	
.col-2
	width: 15.4166666667%
.col-3
	width: 23.875%
.col-4
	width: 32.3333333333%
.col-5
	width: 40.7916666667%
.col-6
	width: 49.25%
.col-7
	width: 57.7083333333%
.col-8
	width: 66.1666666666%
.col-9
	width: 74.625%
.col-10
	width: 83.0833333333%
.col-11
	width: 91.5416666666%
.col-12
	width: 100%
@media only screen and (max-width: 550px)
	.col-1,
	.col-2,
	.col-3,
	.col-4,
	.col-5,
	.col-6,
	.col-7,
	.col-8,
	.col-9,
	.col-10,
	.col-11,
	.col-12
		width: auto
		float: none
	.column + .column
		margin-left: 0
	
